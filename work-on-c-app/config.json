{
	"listeners": {"*:7080": {"pass": "applications/blah"}},
	"applications": {
		"blah": {
			"type": "c",
			"working_directory": "./",
			"prefix": "./blah",
			"cc": "/usr/bin/clang",
			"flags": [
				"-B/usr/bin",
				"-I../src",
				"-I../build/include",
				"-Wall",
				"-Wextra",
				"-Werror"
			]
		}
	}
}
